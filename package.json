{
  "name": "my-filesystem-server",
  "version": "0.1.0",
  "description": "An MCP file system server that actually works.",
  "private": true,
  "type": "module",
  "bin": {
    "my-filesystem-server": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "0.6.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "archiver": "^7.0.1",
    "chardet": "^2.0.0",
    "comment-parser": "^1.4.1",
    "crypto-js": "^4.2.0",
    "doctrine": "^3.0.0",
    "extract-zip": "^2.0.1",
    "file-type": "^19.6.0",
    "glob": "^11.0.0",
    "iconv-lite": "^0.6.3",
    "jsdoc-to-markdown": "^9.1.1",
    "markdown-it": "^14.1.0",
    "mime-types": "^2.1.35",
    "openapi-types": "^12.1.3",
    "swagger-jsdoc": "^6.2.8",
    "typedoc": "^0.27.5"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/crypto-js": "^4.2.2",
    "@types/extract-zip": "^2.0.3",
    "@types/iconv-lite": "^0.0.1",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.17.10",
    "typescript": "^5.7.2"
  }
}
